{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "./assets/icon128-on.png",
         "16": "./assets/icon16-on.png",
         "32": "./assets/icon32-on.png",
         "64": "./assets/icon64-on.png"
      },
      "default_popup": "./popup.html",
      "default_title": "Jam"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Y"
         }
      },
      "toggle-screenshot": {
         "description": "Take screenshot",
         "suggested_key": {
            "default": "Ctrl+Shift+S"
         }
      },
      "toggle-session-replay": {
         "description": "Prompt instant replay",
         "suggested_key": {
            "default": "Ctrl+Shift+X"
         }
      },
      "toggle-video": {
         "description": "Start / stop tab recording",
         "suggested_key": {
            "default": "Ctrl+Shift+Space"
         }
      }
   },
   "description": "The fastest bug reporting tool. Cut your bug reporting time by 20x.",
   "differential_fingerprint": "1.9c397fbcaa6b29330a80c9fd1ad6ba7ced0262a3cab5b64c3951d4f5031a28b5",
   "externally_connectable": {
      "matches": [ "*://*.jam.dev/*", "*://*.jamtest.kitchen/*", "*://*.jam.test/*" ]
   },
   "icons": {
      "128": "./assets/icon128-on.png",
      "16": "./assets/icon16-on.png",
      "32": "./assets/icon32-on.png",
      "64": "./assets/icon64-on.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Jam",
   "optional_permissions": [ "desktopCapture" ],
   "permissions": [ "tabCapture", "webRequest", "webRequestBlocking", "webNavigation", "activeTab", "tabs", "storage", "cookies", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.30.1",
   "web_accessible_resources": [ "assets/*", "injected-scripts/*" ]
}
