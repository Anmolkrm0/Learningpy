{
   "action": {
      "default_icon": "icons/icon_30.png",
      "default_popup": "popup.html",
      "default_title": "Find contacts with SignalHire!"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "css": [ "assets/content.css" ],
      "js": [ "content.js" ],
      "matches": [ "https://*.linkedin.com/*" ],
      "run_at": "document_idle"
   }, {
      "js": [ "contentLI.js" ],
      "matches": [ "https://*.linkedin.com/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "contentSocial.js" ],
      "matches": [ "https://github.com/*", "https://*.facebook.com/*", "https://twitter.com/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "contentSH.js" ],
      "matches": [ "https://www.signalhire.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "default-src 'self'; style-src 'sha256-UkqLW2iCjMAcH5APyL9ClEdxwqu9gNPiwcEEy/1uteM='; connect-src *; img-src 'self' data: media-exp1.licdn.com;"
   },
   "description": "SignalHire is a great way to find contact information in a couple of clicks.",
   "differential_fingerprint": "1.500691b76541cc942d444389212442c60af4786bb62104f3ae083c89a190b768",
   "host_permissions": [ "\u003Call_urls>", "https://www.signalhire.com/*" ],
   "icons": {
      "128": "icons/icon_128.png",
      "16": "icons/icon_16.png",
      "30": "icons/icon_30.png",
      "48": "icons/icon_48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "SignalHire - find email or phone number",
   "permissions": [ "storage", "cookies", "tabs", "scripting", "notifications" ],
   "short_name": "SH",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.1.14",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "assets/*", "inject.js", "injectLI.js" ]
   } ]
}
